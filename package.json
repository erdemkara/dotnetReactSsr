{
  "name": "react-ssr-mvc",
  "version": "1.0.0",
  "description": ".NET 8 MVC with React SSR",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:dotnet\"",
    "dev:next": "cd frontend && npm run dev",
    "dev:dotnet": "dotnet run --project ReactSsrMvc.csproj",
    "build": "cd frontend && npm run build",
    "build:old": "webpack --mode production",
    "watch": "webpack --mode development --watch"
  },
  "keywords": [
    "react",
    "ssr",
    "asp.net",
    "mvc"
  ],
  "author": "",
  "license": "ISC",
  "sideEffects": [
    "*.css",
    "./ClientApp/src/pages/*.tsx",
    "./ClientApp/src/App.tsx"
  ],
  "dependencies": {
    "lucide-react": "^0.555.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.2.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
